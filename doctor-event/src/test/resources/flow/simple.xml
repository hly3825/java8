<?xml version="1.0" encoding="utf-8"?>
<!-- 母猪状态简单流程开始 -->
<workflow key="FemaleSimpleFlow" name="母猪状态简单流程测试">

    <!-- 开始节点 -->
    <start name="开始节点" pointx="50" pointy="100">
        <transition name="开始连线" target="怀孕" handler="breedHandler" describe="配种事件"/>
    </start>

    <!-- 怀孕节点 -->
    <task name="怀孕" assignee="" pointx="250" pointy="100">
        <transition name="任务连线1" target="哺乳" handler="deliverHandler" describe="分娩事件" />
    </task>

    <!-- 哺乳节点 -->
    <task name="哺乳" assignee="" pointx="450" pointy="100">
        <transition name="任务连线2" target="已断奶" handler="weaningHandler" describe="断奶事件" />
    </task>

    <!-- 已断奶节点 -->
    <task name="已断奶" assignee="" pointx="650" pointy="100">
        <transition name="任务连线3" target="结束节点" handler="leaveHandler" describe="离场事件"/>
    </task>

    <!-- 结束节点 -->
    <end name="结束节点" pointx="850" pointy="100" />

</workflow>
